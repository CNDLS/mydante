!!! 5
- MyDante::Application.config.ui.build(self, :page_header, :navigation, :page_footer)

%html
  %head
    %title MyDante
    = stylesheet_link_tag("application")
    = javascript_include_tag("application")
    = javascript_include_tag("menu")
    = csrf_meta_tags
    = yield :head

%body
  %section#powered_by_cndls
    = link_to "powered by CNDLS", "http://cndls.georgetown.edu"
  %section#page
    = yield :page_header
    = yield :navigation
    #page_container
      %article#page_contents
        = render :partial => "application/messages"
        = yield
      = yield :page_footer
      #page_bottom